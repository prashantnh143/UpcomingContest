  body

    .container
      .header
        button(onclick = 'cf()' ) Codeforces 
        button(onclick = 'cc()') Codechef
        button(onclick = 'lc()') Leetcode 
        button(onclick = 'ac()') Atcoder
        button(onclick = 'he()') HackerEarth 
        button(onclick = 'ks()') KickStart


    
  
        
    
    .force
      h1 Codeforces
      table
        tr
          th Contest Name
          th Status
          th Start-Date 
          th Duration
        -var len = 0;
        -if(Codeforces != 'undefined')
          -var len = Codeforces.length;
            
          

          -for(var i=0;i<len;i++){
          tr
            td
              - var url = Codeforces[i].url;
              a(href = url) #{Codeforces[i].name}
            td #{Codeforces[i].status}
            - let dt=new Date(Codeforces[i].start_time);
            td #{dt.toLocaleString('en-AU')} 
            td #{time_convert(Codeforces[i].duration)}
          -}
      



    
