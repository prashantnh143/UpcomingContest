    -function cf(){
      -var x=document.getElementByClassName('force')[0];
      -x.style.display='None';
    -}




    -function time_convert(time){
    - time = time/60;
    - var year = Math.floor(time / 525600);
    - time = time % 525600;
    -var days = Math.floor(time/1440);

    -time = time%1440;

    -var hour = Math.floor(time/60);
    -var minute = time % 60;

    -if(hour <10)
      -hour = '0' + hour;

    -if(minute < 10)
      -minute = '0' + minute;

    -if(days<10)
      - days = '0' + days;



    -var ret = "";

    -if(year >=1){
      -ret = "" + year + " : ";
      -if(days==0)
        -ret+= "00 : ";
    -}

    -if(days>=1)
      -ret += "" + days + " : ";

    -ret += hour + " : " + minute;


    -return ret;
    -}


  
     

    
    
    
    
    